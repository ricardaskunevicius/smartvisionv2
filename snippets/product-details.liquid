<style>
  ul.tabs {
    border-bottom: 1px solid #DDDDDD;
    display: block;
    margin: 0 0 20px;
    padding: 0;
  }
  ul.tabs li {
    display: block;
    float: left;
    height: 30px;
    margin-bottom: 0;
    padding: 0;
    width: auto;
  }
  ul.tabs li a {
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #F5F5F5;
    border-color: #DDDDDD !important;
    border-style: solid;
    border-width: 1px 1px 0 1px;
    display: block;
    font-size: 13px;
    height: 29px;
    line-height: 30px;
    margin: 0;
    padding: 0 20px;
    text-decoration: none;
    width: auto;
    color: #303030;
    border-bottom: none !important;
  }
  ul.tabs li a.active {
    background: none repeat scroll 0 0 #FFFFFF;
    border-left-width: 1px;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    color: #111111;
    height: 30px;
    margin: 0 0 0 -1px;
    padding-top: 4px;
    position: relative;
    top: -4px;
  }
  ul.tabs li:first-child a.active {
    margin-left: 0;
  }
  ul.tabs li:first-child a {
    border-top-left-radius: 2px;
    border-width: 1px 1px 0;
  }
  ul.tabs li:last-child a {
    border-top-right-radius: 2px;
  }
  ul.tabs:before,
  ul.tabs:after {
    content: " ";
    display: block;
    height: 0;
    overflow: hidden;
    visibility: hidden;
    width: 0;
  }
  ul.tabs:after {
    clear: both;
  }

  .tab-content {
    display: none;
  }

  .tab-content a {
    font-size: 13px;
  }

  .tab-content .table tr:first-child td {
    border-top: 0;
  }

  .tab-content .table td {
    border-bottom: 0;
  }

  .tab-content .table td.strong {
    font-weight: 500;
  }

  .tab-content h4 {
    margin-bottom: 20px;
  }

  .dimensions {
    position: relative;
    background: #ffffff;
    margin-top: 18px;
    display: flex;
    flex-flow: wrap;
  }

  .dimensions .dimension {
    width: 45%;
    padding: 10px;
    text-align: center;
    height: 140px;
    flex-direction: column;
  }

  .dimensions .dimension img {
    margin-bottom: 5px;
    max-height: 100%;
  }

  .dimensions p {
    margin: 0;
    font-size: 11px;
    line-height: 16px;
    color: #666666;
  }

  .float_div1 div1 {
    width: 33%;
    display: table-cell;
  }

  .float_div {
    padding: -1%;
    margin: 0;
  }

  h6 {
    margin: 0;
    padding: 2px 0;
  }
  .shippingTable {
    border-collapse: collapse;
    border: #111111 1px;
  }
  td,
  th {
    border: #111111 1px solid;
    text-align: center;
    vertical-align: middle;
  }

  @media all and (max-width: 2576px) {

    .float_div {
      width: 100% !important;
      display: unset !important;
    }

  }

  .strong {
    font-weight: bold;
  }
</style>
