                    {% if product.selected_or_first_available_variant.metafields.variant.bridgeWidth %}
                      {{ product.selected_or_first_available_variant.metafields.variant.bridgeWidth }}
                    {% endif %}
                    -
                    {% if product.selected_or_first_available_variant.metafields.variant.sideLength %}
                      {{ product.selected_or_first_available_variant.metafields.variant.sideLength }}
                    {% endif %}(
                    {% if product.selected_or_first_available_variant.metafields.variant.size %}
                      {{ product.selected_or_first_available_variant.metafields.variant.size }}
